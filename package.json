{
  "name": "react-shopper-app",
  "version": "0.1.0",
  "description": "Simple application for testing the basics of React JS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "npm run build:dev",
    "build": "webpack -p && cp src/index.html dist/index.html",
    "build:dev": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot"
  },
  "keywords": [
    "react",
    "e-commerce"
  ],
  "author": "Robin Hood <robinhood.experiment@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
